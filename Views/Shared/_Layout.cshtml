@{ 
    var model = (List<ATZClient.Models.CartItem>)Session["Cart"];
}
<body>
    <!-- LOADER -->
    <div id="preloader">
        <div class="loading_wrap">
            <img src="~/Assest/image/loader_logo.png" alt="logo">
        </div>
    </div>
    <!-- LOADER -->
    <!-- Start Header Section -->
    <header>
        @{ Html.RenderAction("Header", "Home");}
        <div class="header-mdl">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="hm-inner d-sm-flex align-items-center justify-content-between">
                            <div class="header-logo">
                                <a href="index.html"><img src="~/Assest/image/logo.png" alt="logo"></a>
                            </div>
                            <form class="header-form">
                                <input class="search-box" placeholder="Search Product..." required value="" type="search">
                                <button type="submit">Search</button>
                            </form>
                            <div class="header-right">
                                <div class="header-cart" >
                                    <a href="#">
                                        <div class="cart-icon">
                                            <img src="~/Assest/image/cart-icon.png" alt="cart-icon">
                                            <span id="txtCartTotal_localStorage"></span>                                             
                                            @if (model != null)
                                            {
                                                <span>@Session["TotalProduct"]</span>
                                            }
                                        </div>
                                        $<i class="fa fa-angle-down"></i>
                                        @if (Session["TotalMoney"] == null)
                                        {
                                            <span>0</span>
                                        }
                                        else
                                        {
                                            @Session["TotalMoney"]
                                        }
                                    </a>
                                    <div class="cart-box" >
                                        <div class="cart-info" id="cart">                                          
                                            @if (model != null)
                                            {
                                                foreach (var item in model)
                                                {
                                                    <div class="cart-prodect d-flex">
                                                        <div class="cart-img">
                                                            <img src="~/Assest/image/@item.ImageUrl" alt="cart-img">
                                                        </div>
                                                        <div class="cart-product">
                                                            <a href="#">@item.Name</a>
                                                            <p>$ @item.Price &emsp; @item.Quantily</p>
                                                        </div>
                                                        <a href="/Home/RemoveCart?ID=@item.ID" class="close-icon d-flex align-items-center"><i class="ion-close"></i></a>
                                                    </div>
                                                }
                                            }
                                        </div>
                                        <div class="price-prodect d-flex align-items-center justify-content-between">
                                            <p class="total">Total: @(Session["TotalProduct"])</p>
                                            <p class="total-price">$ @(Session["TotalMoney"] == null ? 0 : Session["TotalMoney"])</p>
                                        </div>
                                        <div class="cart-btn">
                                            <a href="@Url.Action("ViewCart","Home")" class="btn btn-primary">View Cart</a>
                                        </div>
                                    </div>
                                </div>
                                <div class="d-lg-none mm_icon">
                                    <div class="form-captions" id="search">
                                        <button type="submit" class="submit-btn-2"><i class="fa fa-search"></i></button>
                                    </div>
                                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"><i class="fa fa-bars"></i></button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        @{ Html.RenderAction("HeaderMenu", "Home");}
    </header>
    <!-- End Header Section -->
